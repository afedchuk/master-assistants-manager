{
  "name": "afedchuk/assistant-manager",
  "description": "The package allow to manage user assistants(attach, detach new assistant, share information with assistant)",
  "keywords": ["Users","Accounts","User Assistants"],
  "authors": [
    {
      "name": "Andrii Fedchuk",
      "email": "andriy.fedchuk@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable" : true,
  "require": {
    "php": ">=5.6.0",
    "laravel/framework": "~5.5"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.1",
    "illuminate/database": "~5.0"
  },
  "autoload": {
    "psr-0": {
      "Afedchuk\\AssistantManager": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Afedchuk\\AssistantManager\\AssistantServiceProvider"
      ]
    }
  }
}
